{
  "name": "hellobooks",
  "version": "1.0.5",
  "description": "Simple Online Book Library",
  "scripts": {
    "install:config": "npm prune && npm install",
    "lint": "markdownlint --config linters/.markdownlint.json",
    "release": "babel src --out-dir lib",
    "pretest": "npm run --silent lint && sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "migrate:test": "sequelize db:migrate --env test",
    "migrate": "sequelize db:migrate",
    "build": "babel server -d build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "NODE_ENV=test mocha --compilers js:babel-register",
    "start:dev": "nodemon ./server/index.js --exec babel-node --presets es2015,stage-2",
    "start": "babel-node ./server/index.js",
    "server": "nodemon --exec babel-node -- ./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fidelisojeah/Hello-Books.git"
  },
  "keywords": [
    "style guide",
    "lint",
    "airbnb",
    "es6",
    "es2015",
    "react",
    "jsx"
  ],
  "author": "Fidelis Ojeah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fidelisojeah/Hello-Books/issues"
  },
  "homepage": "https://github.com/fidelisojeah/Hello-Books",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "coveralls": "~2.7.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "prettier-eslint": "^6.4.2",
    "sinon": "~1.8.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "cookie-encrypter": "^0.2.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "eslint-config-airbnb": "^15.1.0",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "markdownlint-cli": "^0.3.1",
    "morgan": "^1.8.2",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.8.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
