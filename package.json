{
  "name": "hellobooks",
  "version": "1.0.5",
  "description": "Simple Online Book Library",
  "scripts": {
    "install:config": "npm prune && npm install",
    "lint": "markdownlint --config linters/.markdownlint.json",
    "pretest": "npm run --silent lint",
    "test": "NODE_ENV=test mocha --compilers js:babel-register",
    "test:config": "cd packages/eslint-config-airbnb; npm test",
    "test:config:base": "cd packages/eslint-config-airbnb-base; npm test",
    "start:dev": "nodemon ./bin/www --exec babel-node --presets es2015,stage-2",
    "build": "babel lib -d dist --presets es2015,stage-2",
    "serve": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fidelisojeah/Hello-Books.git"
  },
  "keywords": [
    "style guide",
    "lint",
    "airbnb",
    "es6",
    "es2015",
    "react",
    "jsx"
  ],
  "author": "Fidelis Ojeah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fidelisojeah/Hello-Books/issues"
  },
  "homepage": "https://github.com/fidelisojeah/Hello-Books",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "markdownlint-cli": "^0.3.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "prettier-eslint": "^6.4.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "eslint-config-airbnb": "^15.1.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
